{
  "Inline Math": {
    "prefix": "im",
    "body": [
      "$${1}$${0}"
    ],
    "description": "Add inline math"
  },
  "Displayed Math": {
    "prefix": "dm",
    "body": [
      "$$${1}$$${0}"
    ],
    "description": "Add displayed math"
  },
  "[Anki] Basic Card": {
    "prefix": "basic",
    "body": [
      "START",
      "Basic",
      "Front: ${1}",
      "Back: ${2}",
      "END$0"
    ],
    "description": "Setup basic anki card"
  },
  "[Anki] Cloze Card": {
    "prefix": "cloze",
    "body": [
      "START",
      "Cloze",
      "${1}",
      "END${0}"
    ],
    "description": "Setup cloze anki card"
  },
  "[Anki] Cloze": {
    "prefix": "cl",
    "body": [
      "{{c${2:1}::${1:${TM_SELECTED_TEXT}}}}${0}"
    ],
    "description": "Setup cloze mark"
  },
  "[Anki] Cloze Overlapper": {
    "prefix": "clozo",
    "body": [
      "START",
      "Cloze Overlapper",
      "${1}",
      "{{c99::ask-all}}",
      "Before|After|OnlyContext|RevealAll|InactiveHints: ${2}",
      "END${0}"
    ],
    "description": "Setup cloze overlapper anki card"
  }
}
